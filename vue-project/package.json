{
  "name": "vue-project",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "openapi:make:config": "node .scripts/openapitools.generator.js",
    "openapi:make:schema2ts": "json2ts -i ./node_modules/@openapitools/openapi-generator-cli/config.schema.json -o ./.openapitools/openapitools.d.ts --unreachableDefinitions",
    "openapi:make:client": "openapi-generator-cli generate",
    "openapi:postmake": "node .scripts/openapitools.postmake.mjs",
    "openapi:make": "dotenvx run -f .env.project -- run-s openapi:make:config openapi:make:client openapi:postmake",
    "favicons:make": "dotenvx run -f .env.project -- node .scripts/favicon.generator.mjs",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "predocs": "typedoc",
    "docs:dev": "vitepress dev docs --port 4305",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "pinia": "^2.1.7",
    "primevue": "^3.52.0",
    "vue": "^3.4.27",
    "vue-router": "^4.3.3"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^0.44.1",
    "@openapitools/openapi-generator-cli": "^2.13.4",
    "@rushstack/eslint-patch": "^1.8.0",
    "@storybook/addon-essentials": "^8.1.8",
    "@storybook/addon-interactions": "^8.1.8",
    "@storybook/addon-links": "^8.1.8",
    "@storybook/blocks": "^8.1.8",
    "@storybook/test": "^8.1.8",
    "@storybook/vue3": "^8.1.8",
    "@storybook/vue3-vite": "^8.1.8",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.14.2",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.57.0",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-vue": "^9.23.0",
    "favicons": "^7.2.0",
    "jsdom": "^24.1.0",
    "npm-run-all2": "^6.2.0",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.6.4",
    "sass": "^1.77.5",
    "storybook": "^8.1.8",
    "tailwindcss": "^3.4.4",
    "tailwindcss-breakpoints-inspector": "^1.1.0",
    "typedoc": "^0.25.13",
    "typedoc-plugin-markdown": "^4.0.3",
    "typedoc-plugin-vue": "^1.1.0",
    "typedoc-vitepress-theme": "^1.0.0",
    "typescript": "~5.4.5",
    "unplugin-auto-import": "^0.17.6",
    "unplugin-vue-components": "^0.27.0",
    "unplugin-vue-router": "^0.9.1",
    "vite": "^5.3.0",
    "vite-plugin-vue-devtools": "^7.2.1",
    "vitepress": "^1.2.3",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.21"
  }
}
